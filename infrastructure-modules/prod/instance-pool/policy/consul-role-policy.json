{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "${config_bucket_arn}/consul",
        "${config_bucket_arn}/consul/*",
        "${cloudinit_bucket_arn}/consul",
        "${cloudinit_bucket_arn}/consul/*"
      ]
    }
  ]
}